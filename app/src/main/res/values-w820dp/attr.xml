<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 下拉输入框 -->
    <declare-styleable name="DropEditText">
        <attr name="drawableRight" format="reference" />
        <attr name="dropMode">
            <enum name="match_parent" value="0" />
            <enum name="wrap_content" value="1" />
        </attr>
        <attr name="hint" format="string|reference" />
    </declare-styleable>
    
    
     <declare-styleable name="CircularImage">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="PxBlurringView">
        <attr name="blurRadius" format="integer"/>
        <attr name="downsampleFactor" format="integer"/>
        <attr name="overlayColor" format="color"/>
    </declare-styleable>


</resources>
